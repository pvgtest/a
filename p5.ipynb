   "source": [
    "import numpy as np\n",
    "\n",
    "X=np.array([[1,1,1,-1],[-1,-1,1,1]])\n",
    "Y=np.array([[1,-1],[-1,1]])\n",
    "W=np.dot(Y.T,X)\n",
    "\n",
    "def bam(x):\n",
    "    return np.sign(np.dot(W,x))\n",
    "\n",
    "x_test=np.array([1,-1,-1,-1])\n",
    "y_test=bam(x_test)\n",
    "\n",
    "print(x_test)\n",
    "print(y_test)\n",
    "\n"
   ]
